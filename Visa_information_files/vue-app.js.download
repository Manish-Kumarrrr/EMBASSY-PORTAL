var json_and_arr = {
  loop_obj: function (obj, fn, keys, end_return_val, extra_fn_param) {
    if (typeof obj != "object" || obj == null) return;

    var return_val,
      are_prevent_keys = false;

    if (keys && typeof keys == "object" && !$.isArray(keys)) {
      are_prevent_keys = keys.are_prevent_keys;
      keys = keys.keys;
    }

    if (!keys || are_prevent_keys) {
      for (var key in obj) {
        if ($.inArray(key, keys) != -1) continue;

        return_val = fn(obj, key, obj[key], extra_fn_param);
        if (return_val != undefined) return return_val;
      }
    } else {
      for (var i = 0; i < keys.length; i++) {
        return_val = fn(obj, keys[i], obj[keys[i]], extra_fn_param);
        if (return_val != undefined) return return_val;
      }
    }

    return end_return_val;
  },
  check_value: (function () {
    var in_any_or_all_key = (function () {
        var data = {},
          get_prop_value = function (prop, main_obj) {
            return prop ? main_obj[prop] : main_obj;
          },
          check_details = {
            in_all_key: {
              fn: function (obj, key) {
                if (
                  obj[key] === undefined ||
                  get_prop_value(data.prop, obj[key]) !== data.value
                )
                  return false;
              },
              end_return_val: true,
            },
            in_any_key: {
              fn: function (obj, key) {
                if (
                  obj[key] != undefined &&
                  get_prop_value(data.prop, obj[key]) === data.value
                )
                  return true;
              },
              end_return_val: false,
            },
          };

        return function (obj, prop, value, keys, check_key) {
          var cur_check_details = check_details[check_key];

          data.prop = prop;
          data.value = value;

          return json_and_arr.loop_obj(
            obj,
            cur_check_details.fn,
            keys,
            cur_check_details.end_return_val
          );
        };
      })(),
      in_all_key = function (obj, prop, value, keys) {
        return in_any_or_all_key(obj, prop, value, keys, "in_all_key");
      },
      in_any_key = function (obj, prop, value, keys) {
        return in_any_or_all_key(obj, prop, value, keys, "in_any_key");
      },
      in_arr_of_obj = function (arr, key_name, value) {
        for (var i = 0; i < arr.length; i++) {
          if (arr[i][key_name] === value) return i;
        }

        return -1;
      },
      in_obj_of_arr = function (obj, value) {
        for (var key in obj) {
          if ($.isArray(obj[key]) && $.inArray(value, obj[key]) != -1)
            return key;
        }

        return null;
      };

    return {
      in_all_key: in_all_key,
      in_any_key: in_any_key,
      in_arr_of_obj: in_arr_of_obj,
      in_obj_of_arr: in_obj_of_arr,
    };
  })(),
  is_equal: (function () {
    var has_all_equal_value = (function () {
      var loop_fn = function (obj, key, value, data) {
        if (value === data.obj2[key]) return;

        if (
          !(data.deep
            ? json_and_arr.is_equal(
                value,
                data.obj2[key],
                data.also_deep_keys ? data.keys : null,
                data.deep,
                data.also_deep_keys
              )
            : false)
        )
          return false;
      };

      return function (obj1, obj2, keys, deep, also_deep_keys) {
        var param = {
          obj2: obj2,
          keys: keys,
          deep: deep,
          also_deep_keys: also_deep_keys,
        };

        return json_and_arr.loop_obj(obj1, loop_fn, keys, true, param);
      };
    })();

    return function (obj1, obj2, keys, deep, also_deep_keys) {
      if (typeof keys == "boolean") {
        deep = keys;
        keys = undefined;
      }

      also_deep_keys = also_deep_keys || false;

      return typeof obj1 == "object" && typeof obj2 == "object"
        ? has_all_equal_value(obj1, obj2, keys, deep, also_deep_keys) &&
            Boolean(
              keys ||
                has_all_equal_value(obj2, obj1, keys, deep, also_deep_keys)
            )
        : false;
    };
  })(),
  index_of_obj_in_arr: function (arr, obj, keys, deep, also_deep_keys) {
    for (var i = 0; i < arr.length; i++) {
      if (json_and_arr.is_equal(arr[i], obj, keys, deep, also_deep_keys))
        return i;
    }

    return -1;
  },
};

function get_selected_obj_arr_computed_fn(obj_arr_key, keys_arr_key) {
  return function () {
    if (!this[keys_arr_key]) return null;

    var selected_obj_arr = [];

    for (var i = 0; i < this[obj_arr_key].length; i++) {
      if (this[keys_arr_key].indexOf(this[obj_arr_key][i].key) != -1)
        selected_obj_arr.push(this[obj_arr_key][i]);
    }

    return selected_obj_arr;
  };
}

