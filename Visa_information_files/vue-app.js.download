      if (!this.is_initial_selected) return null;
      for (var i = 0; i < this.selected_main_set.length; i++) {
        if (this.selected_main_set[i].hasOwnProperty(this.visaType)) {
          var sel_vis_type = this.selected_main_set[i];
          sel_vis_type = sel_vis_type[this.visaType];
          sel_vis_type = this.fee_sets[sel_vis_type];
          for (key in this.entry_types) {
            if (sel_vis_type.hasOwnProperty(key)) {
              short_arr[key] = sel_vis_type[key];
            }
          }
        }
      }
      return short_arr;
    },
    sel_processing_times: function () {
      this.selected_processing_time = "";
      var sel_pt;
      if (!this.sel_entry_types) return null;
      else if (this.selected_entry_type != "") {
        sel_pt = this.sel_entry_types[this.selected_entry_type];
      }
      return sel_pt;
    },
    selected_processing_fee: function () {
		if (this.visaType === "tourist") {
        if (this.selected_entry_type === "single"){
          if (this.selected_processing_time == "short_1_month" || this.selected_processing_time == "short_6_month"){this.icwf_fee = 0;}else{this.icwf_fee = 3;}
          }
        else {this.icwf_fee = 3;}
      }
      if (!this.selected_processing_time) return null;

      return this.sel_processing_times[this.selected_processing_time];
    },

    total_fee: function () {
      if (this.selected_processing_fee == null) return null;

      return (
        this.selected_processing_fee + this.icwf_fee + this.vfs_service_fee
      );
    },
  },
  watch: {
    cur_entry_type_fee_set: function (val) {
      this.selected_processing_time = this.is_in_cur_entry_type_fee_set(
        this.selected_processing_time
      )
        ? this.selected_processing_time
        : "";
    },
    selected_country_index: function (val) {
      this.reset();
    },
    selected_visa_type: function (val) {
      this.reset();
    },
  },
  methods: {
    is_in_cur_entry_types: function (entry_type_key) {
      return (
        this.cur_entry_types &&
        this.cur_entry_types.indexOf(entry_type_key) != -1
      );
    },
    is_in_cur_entry_type_fee_set: function (processing_time_key) {
      return (
        this.cur_entry_type_fee_set &&
        this.cur_entry_type_fee_set.hasOwnProperty(processing_time_key)
      );
    },
    reset: function () {
      this.selected_entry_type = "";
      this.selected_processing_time = "";
      this.selected_delivery_fee = null;
    },
  },
});

new Vue({
  el: "#visa_fee_form",
});
}
