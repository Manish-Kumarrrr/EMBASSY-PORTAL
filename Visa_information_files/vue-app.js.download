      var fee_sets = this.fee_sets,
        fee_set_obj,
        entry_type_obj,
        cur_type2_fee_set,
        type2_fee_sets = {};

      for (var fee_set_key in fee_sets) {
        fee_set_obj = fee_sets[fee_set_key];

        type2_fee_sets[fee_set_key] = cur_type2_fee_set = {};

        for (var entry_type_key in fee_set_obj) {
          entry_type_obj = fee_set_obj[entry_type_key];

          for (var processing_time_key in entry_type_obj) {
            cur_type2_fee_set[processing_time_key] =
              cur_type2_fee_set[processing_time_key] || {};

            cur_type2_fee_set[processing_time_key][entry_type_key] =
              entry_type_obj[processing_time_key];
          }
        }
      }

      return type2_fee_sets;
    },
  },
};

